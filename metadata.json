{
  "name": "bodgit-rtadvd",
  "version": "1.0.0",
  "author": "bodgit",
  "summary": "Puppet Module for managing the IPv6 route advertisement daemon",
  "license": "Apache-2.0",
  "source": "https://github.com/bodgit/puppet-rtadvd",
  "project_page": "https://github.com/bodgit/puppet-rtadvd",
  "issues_url": "https://github.com/bodgit/puppet-rtadvd/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "5.7",
        "5.8",
        "5.9"
      ]
    },
    {
      "operatingsystem": "RedHat",
