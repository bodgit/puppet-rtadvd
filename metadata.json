    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">=2.1.0 <3.0.0"
    },
    {
      "name": "richardc/datacat",
      "version_requirement": ">=0.6.1 <1.0.0"
    },
    {
      "name": "herculesteam/augeasproviders_sysctl",
      "version_requirement": ">=2.0.2 <3.0.0"
    }
  ]
